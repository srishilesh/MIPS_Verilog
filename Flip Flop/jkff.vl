module jkff(q,q1,j,k,c,reset);
    output reg q,q1;
    input j,k,c,reset;
    initial begin q=1'b0;q1=1'b1;end
    always @ (posedge c)
    begin
        if (reset) q<=1'b0; else
        case({j,k})
            {1'b0,1'b0}: begin q=q;q1=q1; end
            {1'b0,1'b1}: begin q=q;q1=1'b0; end
            {1'b1,1'b0}: begin q=q;q1=1'b1; end
            {1'b1,1'b1}: begin q=~q;q1=~q1; end
        endcase
        
    end
endmodule
